{
  "name": "lib-js",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/lib-js/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/lib-js",
        "main": "libs/lib-js/src/index.ts",
        "tsConfig": "libs/lib-js/tsconfig.lib.json",
        "assets": ["libs/lib-js/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/lib-js/jest.config.ts"
      }
    }
  }
}
